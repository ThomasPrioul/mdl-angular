{
  "name": "mdl-angular/ui-timewindow",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mdl-angular/ui-timewindow/src",
  "prefix": "mdl",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build-css": {
      "executor": "nx:run-commands",
      "options": {
        "command": "postcss ./libs/mdl-angular/ui-timewindow/src/lib/styles/index.css -o ./libs/mdl-angular/ui-timewindow/src/lib/styles/generated.css --env production"
      }
    },
    "build": {
      "dependsOn": ["build-css"],
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/mdl-angular/ui-timewindow/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mdl-angular/ui-timewindow/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mdl-angular/ui-timewindow/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
